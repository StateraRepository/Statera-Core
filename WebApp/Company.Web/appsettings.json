{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ClientAppSettings": {
    "stsServer": "http://localhost:5010",
    "redirecturl": "http://localhost:5003",
    "clientid": "angularclient",
    "responsetype": "id_token token",
    "scope": "openid profile role accountapi",
    "postlogoutredirecturi": "http://localhost:5003",
    "startchecksession": false,
    "silentrenew": false,
    "startuproute": "/contact-view",
    "forbiddenroute": "/forbidden",
    "unauthorizedroute": "/unauthorized",
    "logconsolewarningactive": true,
    "logconsoledebugactive": true,
    "maxidtokeniatoffsetallowedinseconds": 70,
    "overridewellknownconfiguration": true,
    "overridewellknownconfigurationurl": "http://localhost:5010/.well-known/openid-configuration",
    "apiServer": "http://localhost:52018/",
    "apiFileServer": ""
  }
}
